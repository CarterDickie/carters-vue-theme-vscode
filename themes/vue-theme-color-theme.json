{
  "$schema": "vscode://schemas/color-theme",
  "name": "vue-theme",
  "type": "dark",
  "colors": {
    "panel.background": "#002b36",
    "panelTitle.activeBorder": "#19f9d8",
    "focusBorder": "#19f9d759",
    "notification.background": "#002b36",
    "notification.buttonBackground": "#19f9d8",
    "notification.buttonForeground": "#002b36",
    "notification.buttonHoverBackground": "#19f9d7d3",
    "notification.infoBackground": "#002b36",
    "activityBarBadge.background": "#404954",
    "activityBarBadge.foreground": "#ffffff",
    "input.background": "#002b36",
    "inputValidation.infoBackground": "#002b36",
    "input.border": "#19f9d8",
    "inputValidation.infoBorder": "#19f9d8",
    "inputOption.activeBorder": "#19f9d8",
    "inputValidation.errorBorder": "#19f9d8",
    "activityBar.background": "#002b36",
    "activityBar.foreground": "#19f9d8",
    "editor.background": "#002b36",
    "editorCursor.foreground": "#F8F8F0",
    "editor.foreground": "#E6E6E6",
    "editorWhitespace.foreground": "#34383D",
    "editor.lineHighlightBackground": "#434c57",
    "editor.selectionBackground": "#46494d87",
    "list.hoverBackground": "#09698180",
    "list.hoverForeground": "#d4d4d4",
    "list.focusBackground": "#09698180",
    "list.focusForeground": "#d4d4d4",
    "list.activeSelectionBackground": "#09698180",
    "list.activeSelectionForeground": "#d4d4d4",
    "list.inactiveSelectionBackground": "#002b36",
    "list.inactiveSelectionForeground": "#aeb1b0",
    "list.highlightForeground": "#aeb1b0",
    "list.inactiveFocusBackground": "#002b36",
    "list.inactiveFocusForeground": "#d4d4d4",
    "sideBar.background": "#002b36",
    "sideBar.foreground": "#aeb1b0",
    "sideBar.border": "#4049549d",
    "sideBarTitle.foreground": "#aeb1b0",
    "sideBarSectionHeader.foreground": "#aeb1b0",
    "titleBar.activeBackground": "#002b36",
    "titleBar.activeForeground": "#aeb1b0",
    "titleBar.inactiveBackground": "#002b36",
    "titleBar.inactiveForeground": "#aeb1b0",
    "tab.activeBackground": "#002b36",
    "tab.activeForeground": "#d4d4d4",
    "tab.activeBorder": "#19f9d8",
    "tab.inactiveBackground": "#002b36",
    "tab.inactiveForeground": "#717272",
    "extensionButton.prominentForeground": "#ffffffd8",
    "extensionButton.prominentBackground": "#19f9d7c7",
    "extensionButton.prominentHoverBackground": "#19f9d8",
    "editorWidget.background": "#002b36",
    "statusBar.background": "#002b36",
    "statusBar.foreground": "#19f9d8",
    "statusBar.debuggingBackground": "#002b36",
    "statusBar.noFolderBackground": "#002b36",
    "button.background": "#19f9d8",
    "button.foreground": "#002b36",
    "button.hoverBackground": "#19f9d7c7",
    "dropdown.background": "#002b36",
    "dropdown.border": "#19f9d8",
    "debugToolBar.background": "#002b36",
    "editorGroupHeader.tabsBackground": "#002b36",
    "textLink.activeForeground": "#b3b8b6e5",
    "textLink.foreground": "#00abc1de"
  },
  "tokenColors": [
    {
      "name": "comentarios",
      "scope": "comment",
      "settings": {
        "foreground": "#8a8787",
        "fontStyle": "italic"
      }
    },
    {
      "name": "const, let, import",
      "scope": "storage.type, keyword.control.import.js, keyword.control.from.js",
      "settings": {
        "foreground": "#FF2C6D"
      }
    },
    {
      "name": "string between quotes and template literals",
      "scope": "string.quoted.double, string.template",
      "settings": {
        "foreground": "#00abc1da"
      }
    },
    {
      "name": "color quotes double, simple and template",
      "scope": "punctuation.definition.string.begin, punctuation.definition.string.end, punctuation.definition.string.template.begin, punctuation.definition.string.template.end",
      "settings": {
        "foreground": "#00acc1"
      }
    },
    {
      "scope": "meta.brace.square.js, meta.brace.round.js, punctuation.terminator.statement.js",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "name": "{} , ()",
      "scope": "punctuation.separator.comma.js, punctuation.definition.block.js, punctuation.definition.parameters.begin.js, punctuation.definition.parameters.end.js",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "scope": "keyword.control.loop",
      "settings": {
        "foreground": "#079ba8"
      }
    },
    {
      "scope": "keyword.control.flow, keyword.operator.new",
      "settings": {
        "foreground": "#00e5ffcc"
      }
    },
    {
      "name": "template expression ${} and comparation",
      "scope": "punctuation.accessor, keyword.operator.arithmetic, keyword.operator.comparison, keyword.operator.assignment, keyword.operator.relational, keyword.operator, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end,",
      "settings": {
        "foreground": "#FFCC95"
      }
    },
    {
      "scope": "keyword.control.switch",
      "settings": {
        "foreground": "#079ba8"
      }
    },
    {
      "name": "sigle string quote",
      "scope": "string.quoted.single",
      "settings": {
        "foreground": "#00abc1de"
      }
    },
    {
      "name": "this",
      "scope": "variable.language.this, constant.language.boolean.true, constant.language.boolean.false, constant.language.undefined, constant.language.null",
      "settings": {
        "foreground": "#f48fb1dc"
      }
    },
    {
      "name": "variable object",
      "scope": "variable.other.object.property, variable.other.property, meta.template.expression, support.variable.property, variable.language.super",
      "settings": {
        "foreground": "#ff2c6bf1"
      }
    },
    {
      "name": "keys",
      "scope": "meta.embedded.line, variable.other.object, variable.other.readwrite, entity.name.type.class, entity.name.type,  entity.other.inherited-class, variable.parameter",
      "settings": {
        "foreground": "#a1d7da"
      }
    },
    {
      "name": "number",
      "scope": "constant.numeric.decimal",
      "settings": {
        "foreground": "#ff5622de"
      }
    },
    {
      "name": "console.log",
      "scope": "support.class.console, support.function.console, meta.object-literal.key",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "name": "function and if/else",
      "scope": " support.variable.dom, storage.modifier, support.function",
      "settings": {
        "foreground": "#079ba8"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#09cbdd"
      }
    },
    {
      "scope": " storage.type.function",
      "settings": {
        "foreground": "#f48fb1"
      }
    },
    {
      "name": "json config",
      "scope": "support.type.property-name.json, punctuation.support.type.property-name.begin.json, punctuation.support.type.property-name.end.json",
      "settings": {
        "foreground": "#FFCC95"
      }
    },
    {
      "name": "if else",
      "scope": "keyword.control.conditional",
      "settings": {
        "foreground": "#09cbdd"
      }
    },
    {
      "scope": "support.function.dom",
      "settings": {
        "foreground": "#ffcc95e3"
      }
    },
    {
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#00acc1"
      }
    },
    {
      "scope": "constant.language.json, constant.numeric.json",
      "settings": {
        "foreground": "#ee475e"
      }
    },
    {
      "name": "html tag",
      "scope": "entity.name.tag.script.html, entity.name.tag.structure.any.html, entity.name.tag.html, entity.name.tag.inline.any.html, entity.name.tag.block.any.html, entity.name.tag.other.html, meta.tag.sgml.doctype.html",
      "settings": {
        "foreground": "#ff2c6bf1"
      }
    },
    {
      "name": "src & alt ...",
      "scope": "entity.other.attribute-name.html, entity.other.attribute-name.id.html",
      "settings": {
        "foreground": "#ffcc95ee"
      }
    },
    {
      "name": "gt and lt",
      "scope": "punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "scope": "meta.tag.structure.any.html, meta.tag.inline.any.html",
      "settings": {
        "foreground": "#c5c5c5f1"
      }
    },
    {
      "name": "text html",
      "scope": "text.html.basic",
      "settings": {
        "foreground": "#bdbdbd"
      }
    },
    {
      "scope": "meta.toc-list.id.html",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    // editor config
    {
      "name": "editor config",
      "scope": "keyword.other.definition.ini",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "name": "editor config",
      "scope": "source.ini",
      "settings": {
        "foreground": "#00abc1de"
      }
    },
    // css
    {
      "name": "css",
      "scope": "entity.other.attribute-name.class.css, entity.other.attribute-name.id.css",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "name": "css property",
      "scope": "support.type.property-name.css, support.type.vendored.property-name.css",
      "settings": {
        "foreground": "#079ba8"
      }
    },
    {
      "name": "css value",
      "scope": "support.constant.property-value.css, source.css, support.constant.color.w3c-standard-color-name.css",
      "settings": {
        "foreground": "#90a4ae"
      }
    },
    {
      "scope": "punctuation.section.property-list.begin.bracket.curly.css, punctuation.section.function.begin.bracket.round.css, punctuation.section.function.end.bracket.round.css, punctuation.section.property-list.end.bracket.curly.css, punctuation.section.property-list.begin.bracket.curly.css, punctuation.separator.key-value.css, punctuation.terminator.rule.css",
      "settings": {
        "foreground": "#c2cbcc"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#f06292"
      }
    },
    {
      "name": "color",
      "scope": "meta.function.color.css, constant.other.color.rgb-value.hex.css, support.constant.property-value.css",
      "settings": {
        "foreground": "#09cce2ea"
      }
    },
    {
      "name": "value numeric",
      "scope": "constant.numeric.css, support.function.url.css, support.function.misc.css, punctuation.separator.list.comma.css",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "name": "font css",
      "scope": "support.constant.font-name.css, keyword.other.unit.percentage.css, keyword.other.unit.px.css, keyword.other.unit.rem.css",
      "settings": {
        "foreground": "#f06292"
      }
    },
    {
      "name": "import",
      "scope": "keyword.control.at-rule.import.css, meta.property-value.css",
      "settings": {
        "foreground": "#f06292"
      }
    },
    {
      "name": "string link",
      "scope": "string.quoted.double.css, constant.character.escape.codepoint.css, entity.other.attribute-name.pseudo-class.css, entity.other.attribute-name.pseudo-element.css, entity.other.attribute-name.css, string.unquoted.attribute-value.css",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    // markdown
    {
      "name": "markdown",
      "scope": "markup.heading.markdown",
      "settings": {
        "foreground": "#ffcc95e7"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#00abc1de"
      }
    },
    {
      "scope": "beginning.punctuation.definition.list.markdown",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    // Vue configs
    {
      "scope": "entity.name.tag.template.html, keyword.control.export.js",
      "settings": {
        "foreground": "#ff2c6bf1"
      }
    },
    {
      "scope": "text.html.vue-html",
      "settings": {
        "foreground": "#bdbdbd"
      }
    },
    {
      "scope": "keyword.control.default.js, support.class.builtin.js, meta.function-call.js",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    // webpack
    {
      "scope": "support.type.object.module.js, string.regexp.js, support.variable.object.node.js",
      "settings": {
        "foreground": "#64ffdbda"
      }
    },
    {
      "scope": "support.variable.object.process.js, variable.other.constant.property.js",
      "settings": {
        "foreground": "#a1d7da"
      }
    }
  ]
}
